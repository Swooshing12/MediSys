<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:MediSys"
             xmlns:local2="clr-namespace:MediSys.Converters"
             x:Class="MediSys.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <local2:InvertedBoolConverter x:Key="InvertedBoolConverter" />
            <local2:IsNotNullConverter x:Key="IsNotNullConverter" />
            <local2:EqualConverter x:Key="EqualConverter" />
            <local2:SexoToIndexConverter x:Key="SexoToIndexConverter" />
            <local2:IntToBoolConverter x:Key="IntToBoolConverter" />
            <local2:InverseBoolConverter x:Key="InverseBoolConverter" />
            <local2:CountToInverseBoolConverter x:Key="CountToInverseBoolConverter"/>
            <local2:BoolToAvailabilityTextConverter x:Key="BoolToAvailabilityTextConverter"/>
            <local2:ObjectToBoolConverter x:Key="ObjectToBoolConverter"/>
            <local2:NullToBoolConverter x:Key="NullToBoolConverter"/>
            <local2:CountToBoolConverter x:Key="CountToBoolConverter"/>
            <local2:StringEqualToBoolConverter x:Key="StringEqualToBoolConverter"/>
            <local2:BoolToStringConverter x:Key="BoolToStringConverter"/>
            <local2:OrBoolConverter x:Key="OrBoolConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
